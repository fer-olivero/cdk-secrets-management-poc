{
  "name": "cdk-secrets-management-poc",
  "version": "0.1.0",
  "githubRepository": "service-notifications-email-templates",
  "githubOrganization": "DiligentCorp",
  "type": "module",
  "main": "src/index.js",
  "private": true,
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "type-check": "tsc",
    "test:unit": "tsx --test --test-reporter=spec $(glob '**/*.test.ts')",
    "test:service": "tsx --test --test-reporter=spec $(glob '**/*test.service.ts')",
    "test:storybook": "screener-storybook --conf screener.config.cjs",
    "test": "tsx --test --test-reporter=spec",
    "lint": "eslint .",
    "format": "prettier --write './**/*.ts'",
    "scan-deps": "pnpm audit --audit-level moderate --prod",
    "all-checks": "run-p type-check lint test:unit scan-deps",
    "dev-push-event": "./bin/dev-push-event.ts",
    "deploy": "pnpm cdk deploy --all",
    "watch": "pnpm cdk watch --all",
    "destroy": "pnpm cdk destroy --all",
    "deploy-service-tests": "pnpm cdk deploy -a ./bin/cdk-scoped-service-tests.ts --all",
    "watch-service-tests": "pnpm cdk watch -a ./bin/cdk-scoped-service-tests.ts --all",
    "destroy-service-tests": "pnpm cdk destroy -a ./bin/cdk-scoped-service-tests.ts --all",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "service-tests": "./bin/service-tests.ts",
    "clean-up-aws-services-for-testing": "pnpm service-tests --purge_SQSs --purge_Buckets",
    "pipeline-run-service-tests": "pnpm clean-up-aws-services-for-testing && pnpm test:service"
  },
  "devDependencies": {
    "@aws-sdk/client-ssm": "^3.357.0",
    "@aws-sdk/s3-request-presigner": "^3.357.0",
    "@aws-sdk/types": "^3.357.0",
    "@babel/core": "^7.22.5",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/blocks": "^7.0.22",
    "@storybook/html": "^7.0.22",
    "@storybook/html-vite": "^7.0.22",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/async-retry": "^1.4.5",
    "@types/aws-lambda": "^8.10.119",
    "@types/html-to-text": "^9.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.11.17",
    "@types/prop-types": "^15.7.5",
    "@types/sanitize-html": "^2.9.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "async-retry": "^1.3.3",
    "aws-cdk": "2.85.0",
    "aws-sdk-client-mock": "^2.1.1",
    "cli-glob": "^0.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "globby": "^13.2.0",
    "jsdom": "^22.1.0",
    "nanoid": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sb": "^7.0.22",
    "screener-storybook": "^1.0.0",
    "storybook": "^7.0.22",
    "tsx": "^3.12.7",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.9.0",
    "@aws-lambda-powertools/tracer": "^1.9.0",
    "@aws-sdk/client-s3": "^3.357.0",
    "@aws-sdk/client-sqs": "^3.357.0",
    "@aws-sdk/credential-providers": "^3.357.0",
    "aws-cdk-lib": "2.90.0",
    "constructs": "^10.2.55",
    "html-to-text": "^9.0.5",
    "i18next": "^22.5.1",
    "nanoid": "^4.0.2",
    "preact": "^10.15.1",
    "preact-render-to-string": "^5.2.6",
    "prop-types": "^15.8.1",
    "rails-timezone": "^1.1.0",
    "react": "npm:@preact/compat",
    "react-bulletproof-button": "^1.0.4",
    "react-dom": "npm:@preact/compat",
    "sanitize-html": "^2.11.0",
    "source-map-support": "^0.5.21",
    "undici": "^5.22.1"
  },
  "pnpm": {
    "overrides": {
      "cls-hooked@4.2.2>semver": "7.5.2",
      "aws-xray-sdk-core@3.4.1>semver": "7.5.2",
      "aws-cdk-lib@2.85.0>semver": "7.5.2"
    }
  }
}
